include _mixins

doctype html
html

  head
    title=data.site.title
    link(href='img/title.png', rel='icon', type='image/x-icon')
    link(href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.4.0/fullcalendar.min.css', rel='stylesheet', type='text/css')
    != css()
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js', type='text/javascript')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js', type='text/javascript')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.4.0/fullcalendar.min.js', type='text/javascript')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.4.0/gcal.min.js', type='text/javascript')
    script(async, defer, src='https://maps.googleapis.com/maps/api/js?key=AIzaSyDhn9Y5Fp0kCaERklzzrKYruISRfvw1URc&callback=initMap')
    != js()
    meta(charset='UTF-8')
    meta(name='description', content='QUT eSports is an eSports centre that includes a dedicated playing space, structured programs for athlete progression and research into gaming.')
    meta(name='keywords', content='qut, esports, gaming, gardens point, qut esports')
    meta(name='author', content='Quintus Cardozo')
    meta(name='viewport', content='width=device-width, initial-scale=1')

  body
    // Website header navigation for mobile
    #mobile-header
      a(href='community.html') Community
      a(href='competition.html') Competition
      a(href='academics.html') Academics
      //span.signup Signup
    #background
    
    block content
